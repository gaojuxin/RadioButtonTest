<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.com.radiobuttontest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name ="example.com.radiobuttontest.chatActivity"></activity>
        <activity android:name="example.com.radiobuttontest.contactsActivity"></activity>
        <activity android:name="example.com.radiobuttontest.discoverActivity"></activity>
        <activity android:name="example.com.radiobuttontest.meActivity"></activity>
    </application>

</manifest>